<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.btbeaconcollector.MainActivity"
    tools:ignore="MergeRootFrame" >

    <Button
        android:id="@+id/btnButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="52dp"
        android:layout_y="31dp"
        android:text="Button" />

    <Button
        android:id="@+id/btnStartMeter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="88dp"
        android:layout_y="342dp"
        android:text="Start Meter" />

    <Button
        android:id="@+id/btnStopMeter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="104dp"
        android:layout_y="400dp"
        android:text="Stop Meter" />

</AbsoluteLayout>
